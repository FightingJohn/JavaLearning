<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>HttpRequest1</servlet-name>
    <servlet-class>com.lyl.request.HttpRequest1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HttpRequest2</servlet-name>
    <servlet-class>com.lyl.request.HttpRequest2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HttpRequest3</servlet-name>
    <servlet-class>com.lyl.request.HttpRequest3</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Test</servlet-name>
    <servlet-class>com.lyl.request.Test</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>HttpRequest1</servlet-name>
    <url-pattern>/servlet/HttpRequest1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HttpRequest2</servlet-name>
    <url-pattern>/servlet/HttpRequest2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HttpRequest3</servlet-name>
    <url-pattern>/servlet/HttpRequest3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Test</servlet-name>
    <url-pattern>/servlet/Test</url-pattern>
  </servlet-mapping>

</web-app>