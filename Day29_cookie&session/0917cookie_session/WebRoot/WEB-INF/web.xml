<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>ShowLoginTime</servlet-name>
    <servlet-class>com.lyl.cookie.ShowLoginTime</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AllProducts</servlet-name>
    <servlet-class>com.lyl.cookie2.AllProducts</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>ShowLoginTime</servlet-name>
    <url-pattern>/servlet/ShowLoginTime</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AllProducts</servlet-name>
    <url-pattern>/servlet/AllProducts</url-pattern>
  </servlet-mapping>

</web-app>