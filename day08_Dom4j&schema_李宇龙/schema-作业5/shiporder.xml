<?xml version="1.0" encoding="ISO-8859-1"?>

<!--根元素shiporder 必须包含属性orderid 其类型为string-->
<shiporder orderid="123456"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="shiporder.xsd">
 <orderperson>FightingJohn</orderperson>
 <shipto>
  <name>wangdao</name>
  <address>longhua</address>
  <city>shenzhen</city>
  <country>china</country>
 </shipto>

 <!--item节点至少有一个， 其包含的note节点可有可无-->
 <item>
  <title>books</title>
  <note>many</note>
  <quantity>3</quantity>
  <price>10.90</price>
 </item>
 <item>
  <title>food</title>
  <quantity>1</quantity>
  <price>9.90</price>
 </item>
</shiporder>